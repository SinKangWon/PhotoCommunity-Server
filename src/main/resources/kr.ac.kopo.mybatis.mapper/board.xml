<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.ac.kopo.photocommunity.mapper.BoardMapper">
    <select id="findByCoordId" resultType="Board">
        SELECT id, title, regdate, member_id, coord_id
        FROM board
        WHERE coord_id = #{coordId}
    </select>
</mapper>